{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "react/jsx-curly-spacing": [2, "always"],
    "react/jsx-no-bind": 0
  },
  "globals": {
    "document": true,
    "it": true,
    "describe": true,
    "beforeEach": true
  }
}